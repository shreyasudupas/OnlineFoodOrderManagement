{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "RedisConfiguration": {
    "Port": 6379,
    "Server": "localhost"
  },
  "AuthenticationConfig": {
    "AUDIENCE": "basketApi,inventoryApi",
    "AUTHORITY": "https://localhost:5005"
  },
  "BasketApiCors": {
    "ORIGIN_URL": "http://localhost:4200",
    "HEADERS": "Content-Type,Authorization,X-CSRF-Token,X-Requested-With,Accept,Accept-Version,Content-Length,Content-MD5,Date,X-Api-Version,X-File-Name",
    "METHODS": "GET,POST,DELETE,PUT"
  }
}
